<div class="span4 offset1">
  <%= simple_form_for current_volunteer, :url => '/htcs/volunteers/update_password' do |f| %>

  <%= f.input :password %>
  <%= f.input :password_confirmation %>

  <%= f.submit 'Save', :class => "btn btn-large btn-primary" %>
  <%= link_to 'Cancel', "/volunteers/sign_in", :class => "btn btn-large" %>
 
  <% end %>
</div>