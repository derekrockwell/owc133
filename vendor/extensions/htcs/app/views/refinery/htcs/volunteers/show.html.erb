<% content_for :body_content_title do %>
  <%= @volunteer.first_name %>
<% end %>

<% content_for :body do %>
  <section>
    <h1>First Name</h1>
    <p>
      <%=raw @volunteer.first_name %>
      <%=raw @volunteer.last_name %>
      
    </p>
  </section>
<% end %>

<% content_for :side_body do %>
  <aside>
    <h2><%= t('.other') %></h2>
    <ul id="volunteers">
      <% @volunteers.each do |volunteer| %>
        <li>
          <%= link_to volunteer.first_name, refinery.htcs_volunteer_path(volunteer) %>
        </li>
      <% end %>
    </ul>
  </aside>
<% end %>

<%= render '/refinery/content_page' %>
