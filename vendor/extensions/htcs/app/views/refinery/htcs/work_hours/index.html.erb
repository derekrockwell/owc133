<% content_for :body do %>
  <h2>Work Hours for <%= current_volunteer.first_name %></h2>
  <ul id="work_hours">
    <% @work_hours.each do |work_hour| %>
      <li>
        <%= link_to work_hour, refinery.htcs_work_hour_path(work_hour) %>
      </li>
    <% end %>
  </ul>
<% end %>

<%= render '/refinery/content_page' %>
